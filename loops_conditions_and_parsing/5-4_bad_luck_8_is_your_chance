#!/usr/bin/env bash
# This script displays "bad luck" on the 4th iteration, "good luck" on the 8th iteration, and "good luck" on the other iterations 
count=1
while [ $count -le 10 ]
do
: count #dummy usage of $count variable

if [ count -eq 4 ]; then
echo "bad luck"

elif [count -eq 8]; then
echo "good luck"

else 
echo "Best School"
fi
count =$((count + 1))
done
